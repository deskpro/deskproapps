{
    "plugins": [
        "alerts",
        "anchors",
        "copy-code-button",
        "codeblock-label",
        "custom-favicon",
        "scripts",
        "toolbar",
        "include-it@git+https://github.com/headzoo/gitbook-plugin-include.git#master",
        "toc"
    ],
